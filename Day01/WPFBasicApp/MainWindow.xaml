<Window x:Class="WPFBasicApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFBasicApp"
        mc:Ignorable="d"
        Title="WpfBasicApp" Height="280" Width="190"
        ResizeMode="NoResize">
    <StackPanel>
        <TextBlock Text="TextBlock" Width="150" Height="30" />
        <Label Content="Label" Width="150" Height="30" />
        <TextBox Text="TextBox" Width="150" Height="30"/>
        <ProgressBar Minimum="0" Maximum="100" Value="10" Width="150" Height="30" />
        <Slider Width="150" Height="30" Minimum="0" Maximum="100" Value="30" />
        <PasswordBox PasswordChar="●" Password="Secret" Width="150" Height="30" />
        <ComboBox Text="Combo1" Width="150" Height="30" />
        <Button Content="Button1" Width="150" Height="30" />
    </StackPanel>

</Window>
